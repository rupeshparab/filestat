<div class="row">
  <div class="col-md-6">
    <p><%= @share.value %></p>
    <textarea id="text" class="kv-preview-data file-preview-text file-zoom-detail" title="<%= @share.value %>" readonly="" style="width: 100%; height: 100%; min-height: 640px;"><%= @share.file_contents %></textarea>
    <br/>
  </div>
  <div class="col-md-6">
    <%= render 'table' %>
    <br/>
  </div>
</div>
<%= render 'commonjs' %>
<script type="text/javascript">
  $(document).ready(function () {
    findStat();
    document.getElementById("bytes").innerText = '<%= @share.size %>';
  });
</script>
